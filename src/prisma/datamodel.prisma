type User {
    id: ID! @id
    createdAt: DateTime! @createdAt
    name: String!
    email: String! @unique
    matricule: String! @unique
    filiere: String! 
    option:String!
    password: String!
    chambers: [Chamber] @relation(name:"ChamberUser")

}

type Chamber {
    id: ID! @id
    createdAt: DateTime! @createdAt
    localisation: String!
    dimension: String!
    quartier: String!
    description: String!
    prix: Int!
    nombreMois: Int
    aDoucheInterne: Boolean
    aCaution: Boolean
    aPlafond: Boolean
    estCarreler: Boolean
    estLibre: Boolean @default(value: true)
    users: [User!]! @relation(name:"ChamberUser")

}
